import formulas.Formula;
import formulas.FormulaFactory;


public class main {

	public static void main(String[] args) {
		

		System.out.println("-(i)------------------------");
		Formula vet[] = new Formula[1000000];
		FormulaFactory ff = new FormulaFactory();
		for (int i = 0; i < vet.length; i++){
			if (i < 2){
				vet[i] = ff.createAtomicFormula("A" + i);
			} else {
				vet[i] = ff.getFormulaComposta((ConectivoBinario) FormulaFactory.getConectivo(TipoConectivo.binario, "||"), vet[i-1], vet[i-2]);
			}
		}
		System.out.println("-(f)------------------------");
	}

}